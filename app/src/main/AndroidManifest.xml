<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cheng.cc.customcontrols">


    <supports-screens
        android:anyDensity="true"
        android:largeScreens="true"
        android:normalScreens="true"
        android:resizeable="true"
        android:smallScreens="true"
        android:xlargeScreens="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!--Study-->
        <activity
            android:name=".activity.CustomStudyAll_Activity"
            android:screenOrientation="portrait" />


        <!--TextView-->
        <activity
            android:name=".activity.CustomTextAll_Activity"
            android:screenOrientation="portrait" />

        <activity android:name=".activity.TextLightning_Activity"
            android:screenOrientation="portrait"/>

        <!--EditText-->
        <activity android:name=".activity.CustomEditAll_Activity"
            android:screenOrientation="portrait"/>

        <activity android:name=".activity.EditDeleteContent_Activity"
            android:screenOrientation="portrait"/>



        <!--Button-->
        <activity android:name=".activity.CustomButtonAll_Activity"
            android:screenOrientation="portrait"/>



        <!--Dialog-->
        <activity android:name=".activity.CustomDialogAll_Activity"
            android:screenOrientation="portrait"/>



    </application>

</manifest>